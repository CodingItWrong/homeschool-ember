<PaperCard>
  <PaperToolbar>
    <PaperToolbarTools>
      <h2>Forms</h2>
    </PaperToolbarTools>
  </PaperToolbar>
  <PaperContent>
    <PaperList>
      {{#each this.model as |form|}}
        <PaperItem>
          <p>
            Form {{form.number}}
          </p>
        </PaperItem>
        <PaperDivider />
      {{/each}}
    </PaperList>
  </PaperContent>
</PaperCard>

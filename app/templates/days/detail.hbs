{{formatted-date @model.date}}

{{#each this.schedulingsGroupedByStudent as |group|}}
  <h2>{{group.student.name}}</h2>

  <table>
    <thead>
      <tr>
        <th>Subject</th>
        <th>Content</th>
      </tr>
    </thead>
    <tbody>
      {{#each group.schedulings as |scheduling|}}
        <tr>
          <td>{{scheduling.content.subject.name}}</td>
          <td>{{scheduling.content.notes}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{/each}}
